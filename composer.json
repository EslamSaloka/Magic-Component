{
    "name": "magic/component",
    "description": "magic component generator",
    "type": "library",
    "version":"1.0",
    "authors": [
        {
            "name": "Eslam Mohsen Handousa",
            "email": "eslammohsenhandousa@gmail.com"
        }
    ],
    "require":{
        "astrotomic/laravel-translatable": "^11.9",
        "mcamara/laravel-localization": "^1.6",
        "proengsoft/laravel-jsvalidation": "^4.5",
        "spatie/laravel-permission": "^5.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6"
    },
    "autoload": {
        "files": [
            "src/app/Helpers/main.php"
        ],
        "psr-4": {
            "Magic\\MagicComponent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magic\\MagicComponent\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Magic\\MagicComponent\\MagicComponentServiceProvider"
            ]
        }
    }
}
